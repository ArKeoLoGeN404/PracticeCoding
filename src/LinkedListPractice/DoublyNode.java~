package LinkedListPractice;

public class DoublyNode {

    public int data;
    public Node next;
    public Node prev;

    public DoublyNode(int data) {
        this.data = data;
        this.next = null;
        this.prev = null;
    }

    public DoublyNode(int data, Node next, Node prev){
        this.data = data;
        this.next = next;
        this.prev = prev;
    }
}
